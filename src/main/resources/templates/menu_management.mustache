                            <article class="thumbnail-classic block-1">
                                <div class="thumbnail-classic-figure"><img src="{{menuImg}}" alt="" width="270" height="250"/>
                                </div>
                                <div class="thumbnail-classic-caption">
                                    <div>
                                        <h5 class="thumbnail-classic-title"><a href="single-product.html">{{menuName}}</a></h5>
                                        <div class="thumbnail-classic-price product-price">{{menuPrice}}원</div>
                                        <div class="thumbnail-classic-button-wrap">
                                            <div class="thumbnail-classic-button"><a type="button" class="button button-primary button-zakaria fl-bigmug-line-note35" href="/edit_menu/{{menuId}}"></a></div>
                                            <div class="thumbnail-classic-button"><button type="button" class="button button-primary button-zakaria fl-bigmug-line-cross81" onclick="delete_menu({{menuId}})"></button></div>
                                        </div>
                                    </div>
                                </div>
                            </article>
